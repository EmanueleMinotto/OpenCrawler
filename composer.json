{
    "autoload": {
        "psr-4": {
            "Bisarca\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bisarca\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Bisarca",
    "license": "MIT",
    "name": "bisarca/bisarca",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "league/container": "^2.2",
        "league/tactician-container": "^1.0",
        "symfony/css-selector": "^2.8 || ^3.0",
        "symfony/dom-crawler": "^2.8 || ^3.0",
        "t1gor/robots-txt-parser": "^0.2.0",
        "tzfrs/googlesitemapparser": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "^5.4",
        "symfony/phpunit-bridge": "^3.1"
    },
    "scripts": {
        "test": [
            "php-cs-fixer fix -vv --ansi",
            "phpunit --colors=always"
        ]
    }
}
