{
    "autoload": {
        "psr-4": {
            "Bisarca\\": "src/Bisarca/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bisarca\\": "tests/Bisarca/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Bisarca",
    "license": [
        "AGPL-3.0",
        "proprietary"
    ],
    "name": "bisarca/bisarca",
    "require": {
        "php": "^7.0",
        "bisarca/robots-txt": "^1.0",
        "guzzlehttp/guzzle": "^6.0",
        "league/container": "^2.2",
        "league/tactician-container": "^1.0",
        "symfony/dom-crawler": "^2.8 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.12",
        "phpunit/phpunit": "^5.5",
        "symfony/phpunit-bridge": "^3.1"
    },
    "scripts": {
        "test": [
            "php-cs-fixer fix -vv --ansi",
            "phpunit --colors=always"
        ]
    }
}
